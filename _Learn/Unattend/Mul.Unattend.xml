					<Order>2</Order>
					<CommandLine>PowerShell -ExecutionPolicy Bypass -File "%SystemDrive%\Yi\Engine\Engine.ps1" -Force</CommandLine>
				</SynchronousCommand>
			</FirstLogonCommands>
